<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#03A9F4"
    tools:context=".Editor">

    <Spinner
        android:id="@+id/sound_spinner3"
        android:layout_width="225dp"
        android:layout_height="40dp"
        android:entries="@array/sound_array"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.036"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/music_spinner"
        app:layout_constraintVertical_bias="0.763" />

    <SeekBar
        android:id="@+id/seekBar3"
        android:layout_width="420dp"
        android:layout_height="40dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toEndOf="@+id/sound_spinner1"
        app:layout_constraintTop_toBottomOf="@+id/music_spinner"
        app:layout_constraintVertical_bias="0.763" />

    <Spinner
        android:id="@+id/sound_spinner2"
        android:layout_width="225dp"
        android:layout_height="40dp"
        android:entries="@array/sound_array"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.036"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/music_spinner"
        app:layout_constraintVertical_bias="0.458" />

    <SeekBar
        android:id="@+id/seekBar2"
        android:layout_width="420dp"
        android:layout_height="40dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toEndOf="@+id/sound_spinner1"
        app:layout_constraintTop_toBottomOf="@+id/music_spinner"
        app:layout_constraintVertical_bias="0.458" />

    <Spinner
        android:id="@+id/music_spinner"
        android:layout_width="691dp"
        android:layout_height="40dp"
        android:entries="@array/music_array"
        android:textAlignment="center"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.088" />

    <Spinner
        android:id="@+id/sound_spinner1"
        android:layout_width="225dp"
        android:layout_height="40dp"
        android:entries="@array/sound_array"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.036"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/music_spinner"
        app:layout_constraintVertical_bias="0.147" />

    <SeekBar
        android:id="@+id/seekBar"
        android:layout_width="420dp"
        android:layout_height="40dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toEndOf="@+id/sound_spinner1"
        app:layout_constraintTop_toBottomOf="@+id/music_spinner"
        app:layout_constraintVertical_bias="0.147" />

    <Button
        android:id="@+id/exit_editor"
        android:layout_width="249dp"
        android:layout_height="52dp"
        android:text="Play"
        android:textSize="24sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/seekBar3"
        app:layout_constraintVertical_bias="0.111" />
</androidx.constraintlayout.widget.ConstraintLayout>